\n    const styles = {\r\n      container: {\r\n        position: \"relative\",\r\n        // top: 0,\r\n        // left: 0,\r\n        zIndex: \"99\",\r\n        opacity: 0.9,\r\n        display: \"flex\",\r\n        alignItems: \"center\",\r\n        // background: \"black\",\r\n        // width: \"100%\",\r\n        color: \"white\",\r\n        fontFamily: \"Lobster\",\r\n      },\r\n      logo: {\r\n        margin: \"0 auto\",\r\n        color: \"cyan\",\r\n        fontSize: \"30px\",\r\n      },\r\n      body: {\r\n        display: \"flex\",\r\n        flexDirection: \"column\",\r\n        alignItems: \"center\",\r\n        width: \"100vw\",\r\n        height: \"100vh\",\r\n        filter: this.state.menuOpen ? \"blur(2px)\" : null,\r\n        transition: \"filter 0.5s ease\",\r\n      },\r\n    };\r\n    const menu = [\"Explore\", \"Became a Creator\", \"Login\", \"Sign Up\"];\r\n    const menuItems = menu.map((val, index) => {\r\n      return (\r\n        <MenuItem\r\n          key={index}\r\n          delay={`${index * 0.1}s`}\r\n          onClick={() => {\r\n            this.handleLinkClick();\r\n          }}\r\n        >\r\n          {val}\r\n        </MenuItem>\r\n      );\r\n    });\r\n\r\n    return (\r\n      <div>\r\n        <div style={styles.container}>\r\n          <MenuButton\r\n            open={this.state.menuOpen}\r\n            onClick={() => this.handleMenuClick()}\r\n            color=\"black\"\r\n          />\r\n          {/* <div style={styles.logo}>Logo</div> */}\r\n        </div>\r\n        <Menu open={this.state.menuOpen}>{menuItems}</Menu>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\n/* MenuItem.jsx*/\r\nclass MenuItem extends React.Component {\r\n  constructor(props) {\r\n    super(props);\r\n    this.state = {\r\n      hover: false,\r\n    };\r\n  }\r\n\r\n  handleHover() {\r\n    this.setState({ hover: !this.state.hover });\r\n  }\r\n\r\n  render() {\r\n    const styles = {\r\n      container: {\r\n        opacity: 1,\r\n        ani